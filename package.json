{
  "name": "mysql-express-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src --out-dir dist --copy-files",
    "start": "node ./dist/main.js",
    "dev": "nodemon --exec babel-node ./src/main.js",
    "model": "sequelize-cli model:generate --name",
    "seed": "sequelize-cli seed:generate --name",
    "migrate:latest": "sequelize-cli db:migrate",
    "migrate:rollback": "sequelize-cli db:migrate:undo:all",
    "migrate:reset": "yarn migrate:rollback && yarn migrate:latest && yarn seed:run",
    "seed:run": "sequelize db:seed:all",
    "seed:down": "sequelize db:seed:undo"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "body-parse": "^0.1.0",
    "chalk": "^4.1.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^2.26.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "http": "^0.0.1-security",
    "joi": "^17.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "nanoid": "^3.1.30",
    "sequelize": "^6.17.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@babel/runtime": "^7.15.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^7.31.0",
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.4.1"
  }
}
